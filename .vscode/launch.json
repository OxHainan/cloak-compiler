    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: scores-check",
            "type": "python",
            "request": "launch",
            "program": "cloak/__main__.py",
            "args": ["check", "./eval-ccs2019/examples/scores/scores.sol"],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: scores-compile",
            "type": "python",
            "request": "launch",
            "program": "cloak/__main__.py",
            "args": ["compile", "-o", "test", "./eval-ccs2019/examples/scores/scores.sol"],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: FuncCalls-check",
            "type": "python",
            "request": "launch",
            "subProcess": true,
            "program": "cloak/__main__.py",
            "args": ["check", "cloak/examples/code/FuncCalls.zkay"],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: FuncCalls-compile",
            "type": "python",
            "request": "launch",
            "program": "cloak/__main__.py",
            "args": ["compile", "-o", "test", "cloak/examples/code/FuncCalls.zkay"],
            "console": "integratedTerminal"
        }
    ]
}